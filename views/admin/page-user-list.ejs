                </header> <!-- card-header end// -->
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>Email</th>
                                    <!-- <th>Status</th> -->
                                    <!-- <th>Registered</th> -->
                                    <th class="text-end"> view</th>
                                </tr>
                            </thead>
                            <tbody>

                                <table class="table table-hover table-vcenter">
                                    <tbody>
                                        <% if (locals.data) { %>
                                            <% users.forEach((user) => { %>
                                                <tr>
                                                  
                                                    <td>
                                                        <%= user.email %>
                                                    </td>
                                                    <td>

                                                        <% if (user.isBlocked) { %>
                                                            <form onsubmit="confirmUnblockUser(event, '<%= user.id %>')" action="/unblock/<%= user.id %>" method="post" style="display:inline;">
                                                              <button type="submit" class="btn btn-danger text-white"><i class="fa fa-lock" aria-hidden="true">unblock</i></button>
                                                            </form>
                                                            
                                                          <% } else { %>
                                                            <form onsubmit="confirmBlockUser(event, '<%= user.id %>')" action="/block-user/<%= user.id %>" method="post" style="display:inline;">
                                                              <button type="submit" class="btn btn-success text-white"><i class="fa fa-unlock" aria-hidden="true">block</i></button>
                                                            </form>
                                                          <% } %>

                                                    </td>
                                                    <td class="text-end">
                                                        <a href="/page-user-detail/<%= user.id %>" class="btn btn-sm btn-brand rounded font-sm mt-15">View details</a>
                                                    </td>
                                                </tr>
                                            <% }); %>
                                        <% } else { %>
                                            <tr>
                                                <td colspan="4">No users found.</td>
                                            </tr>
                                        <% } %>
                                    </tbody>
                                    
                                </table>

                            </tbody>
                        </table> <!-- table-responsive.// -->
