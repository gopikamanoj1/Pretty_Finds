                                    <label class="form-label">Category</label>
                                    <input type="text" name="name" placeholder="Type here" class="form-control" id="category_name" />
                                </div>
                            
                                <!-- New form group for offerPercentage -->
                                <div class="mb-4">
                                    <label class="form-label">Category Offer (Optional)</label>
                                    <input type="number" name="offerPercentage" placeholder="Enter offer percentage" class="form-control" id="category_offer" min="0" max="100">
                                </div>
                            
                                <div class="d-grid">
                                    <button class="btn btn-primary">Create category</button>
                                </div>
                            
                                <!-- Include SweetAlert library -->
                                <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>
                            
                                <% if (locals.message) { %>
                                    <!-- Display SweetAlert for "Category already exists" -->
                                    <script>
                                        Swal.fire({
                                            icon: 'error',
                                            title: 'Category Exists',
                                            text: '<%= message %>',
                                        });
                                    </script>
                                <% } %>
                            
                                <% if (locals.success) { %>
                                    <!-- Display SweetAlert for "Category added successfully" -->
                                    <script>
                                        Swal.fire({
                                            icon: 'success',
                                            title: 'Success',
                                            text: '<%= data %>',
