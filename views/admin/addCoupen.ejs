        <br><br><br>



        <div class="col-lg-12">
            <div class="card mb-4">
                <div class="card-header">
                    <h4>COUPON DETAILS</h4>
                </div>
                <div class="card-body">
                    <div class="table-responsive">
                        <!-- views/couponList.ejs -->

                        <table class="table table-bordered table-hover">
                            <thead class="thead-dark">
                                <tr>
                                    <th scope="col">Sl.No</th>
                                    <th scope="col">Code</th>
                                    <th scope="col">Description</th>
                                    <th scope="col">Discount</th>
                                    <th scope="col">Maximum Amount</th>
                                    <th scope="col">Expiry Date</th>
                                    <th scope="col">Edit</th>
                                    <th scope="col">Delete</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% coupons.forEach((coupon, index) => { %>
                                    <tr>
                                        <th scope="row"><%= index + 1 %></th>
                                        <td><%= coupon.couponCode %></td>
                                        <td><%= coupon.couponDescription %></td>
                                        <td><%= coupon.discountPercentage %>%</td>
                                        <td><%= coupon.maxDiscountAmount %></td>
                                        <td><%= coupon.couponexpiry ? coupon.couponexpiry.toDateString() : 'N/A' %></td>
                                        <td>
                                            <a href="/editCoupon/<%= coupon._id %>" class="btn btn-success">Edit</a>
                                        </td>
                                        <td>
                                            <a href="#" class="delete-link" onclick="confirmDelete('<%= coupon._id %>')" class="btn btn-danger">Delete</a>
                                        </td>
                                    </tr>
                                <% }); %>
                            </tbody>
                        </table>
                        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
                        
                        

                    </div>
                </div>
            </div>
        </div>



        <script>
            function confirmDelete(couponId) {
                // Use SweetAlert to show a confirmation dialog
                Swal.fire({
                    title: 'Are you sure?',
                    text: 'You won\'t be able to revert this!',
                    icon: 'warning',
                    showCancelButton: true,
                    confirmButtonColor: '#d33',
                    cancelButtonColor: '#3085d6',
                    confirmButtonText: 'Yes, delete it!'
                }).then((result) => {
                    if (result.isConfirmed) {
                        // If the user confirms, redirect to the deleteCoupon route
                        window.location.href = `/deleteCoupon/${couponId}`;
                    }
                });
            }
        </script>
        <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

















