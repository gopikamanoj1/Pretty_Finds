        <div class="container form-container">
            <form action="/addCoupon" method="post">
                <div class="mb-4">
                    <div class="row">
                        <div class="col-lg-4 form-group">
                            <label for="couponcode">Coupon code:</label>
                            <input type="text" class="form-control" name="couponcode" id="couponcode">
                            <div id="codeerror" style="color: red;"></div>
                        </div>
                        <div class="col-lg-4 form-group">
                            <label for="coupondiscription">Coupon Discription:</label>
                            <input type="text" class="form-control" name="couponDescription" id="couponDescription">
                            <div id="discerror" style="color: red;"></div>
                        </div>
                        <div class="col-lg-4 form-group">
                            <label for="Discount">Discount:</label>
                            <input type="number" class="form-control" name="Discount" id="Discount">
                            <div id="discounterror" style="color: red;"></div>
                        </div>
                        <div class="col-lg-4 form-group">
                            <label for="MazimumAmount">Maximum Amount:</label>
                            <input type="number" class="form-control" name="MazimumAmount" id="MazimumAmount">
                            <div id="maximumerror" style="color: red;"></div>
                        </div>
                       
                        <div class="col-lg-4 form-group">
                            <label for="couponexpiry">Coupon expiry:</label>
                            <input type="date" class="form-control" name="couponexpiry" id="couponexpiry">
