<section class="content-main">
    <h3  class="align-items-center">Sales Report </h3>
    <div class="card mb-4">
    
        <header class="card-header" id="header">
            <!-- <h4 class="card-title">Latest orders</h4> -->
            <div class="row align-items-center">
                <div class="col-md-3">
                    <form method="post" action="/updateOrdersByDate" id="OrderByDateForm">
                        <div class="row align-items-center">
                            <div class="col-md-10 col-6">
                                <label for="from" class="form-label">From:</label>
                                <input type="date" class="form-control" id="from" name="orderFrom" required>
                                <span id="fromError" style="color: red;"></span>
                            </div>
                            <div class="col-md-10 col-6">
                                <label for="to" class="form-label">To:</label>
                                <input type="date" class="form-control" id="to" name="orderTo" required>
                                <span id="toError" style="color: red;"></span>
                            </div>
                            <div class="col-md-6 col-6" style="padding-top: 4%;">
                                <button class="btn btn-primary" type="submit">Submit</button>
                            </div>
                        </div>
                    </form>
                </div>
                
    
    
                <div class="col-md-2 col-6">
                    <!-- <a href="/dailyOrder" id="dailyButton" class="btn btn-primary btn-sm rounded">Daily</a> -->
                </div>
                <div class="col-md-2 col-6">
                    <!-- <a href="/weeklyOrder" id="MonthlyButton" class="btn btn-primary btn-sm rounded">Weekly</a> -->
                </div>
                <div class="col-md-2 col-6">
                    <!-- <a href="/yearlyOrder" id="YearlyButton" class="btn btn-primary btn-sm rounded">Yearly</a> -->
                </div>
              
    
                <!-- <div class="col-md-2 col-6">
                    <a href="/invoiceSales" class="btn btn-primary btn-sm rounded" >InvoiceReport</a>
                    <button href="#" class="btn btn-primary btn-sm rounded" id="invoice">Invoice</button>
                </div> -->



                
            
        </header>
    
    
    
        <div class="card-body">
            <div class="table-responsive">
                <div class="table-responsive">


                    <table class="table align-middle table-nowrap mb-0" id="example">
                        <thead class="table-light">
                            <tr class="text-center">
                                <th class="align-middle" scope="col">#</th> <!-- Serial Number Column -->
                                <th class="align-middle" scope="col">Order ID</th>
                                <th class="align-middle" scope="col">Billing Name</th>
                                <th class="align-middle" scope="col">Email</th>
                                <th class="align-middle" scope="col">Date</th>
                                <th class="align-middle" scope="col">Order Status</th>
                                <th class="align-middle" scope="col">Amount</th>
                                <th class="align-middle" scope="col">Payment Method</th>
                            </tr>
                        </thead>
                    
                        <tbody>
                            <% if (orders.length > 0) { %>
                                <% orders.forEach((order, index) => { %>
                                    <tr class="text-center">
                                        <td><%= index + 1 %></td> <!-- Serial Number -->
                                        <td><%= order._id %></td>
                                        <% if (order.user) { %>
                                            <td><b><%= order.user.name %></b></td>
                                            <td><%= order.user.email %></td>
                                        <% } else { %>
                                            <td>N/A</td>
                                            <td>N/A</td>
                                        <% } %>
                                        <% if (order.createdAt) { %>
                                            <td><%= order.createdAt.toDateString() %></td>
                                        <% } else { %>
                                            <td>N/A</td>
                                        <% } %>
                                        <td>
                                            <span class="badge rounded-pill <% if (order.status === 'Received') { %>alert-success<% } else if (order.status === 'Shipped') { %>alert-info<% } else { %>alert-warning<% } %>">
                                                <%= order.status %>
                                            </span>
                                        </td>
                                        <td>$<%= order.total !== undefined ? order.total.toFixed(2) : 'N/A' %></td>
                                        <td> <%=order.paymentType %></td>
                                        <td>
                                            <a href="/page-orderDetails/<%= order._id %>" class="btn btn-xs"> View details</a>
                                            
                                        </td>
                                    </tr>
                                <% }); %>
                            <% } else { %>
                                <tr>
                                    <td colspan="8" class="text-center">No sales report available for the specified date range.</td>
                                </tr>
                            <% } %>
                        </tbody>
                         
                    </table>
                    
                   
                </div>
            </div> <!-- table-responsive end// -->
        </div>
    </div>
