                <!-- card-header end// -->
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>#</th>
                                    <th>#ID</th>
                                    <th scope="col">Email</th>
                                    <th scope="col">Total</th>
                                    <th scope="col">Status</th>
                                    <th scope="col">Date</th>
                                    <th scope="col" class="text-end"> Action </th>
                                </tr>
                            </thead>
                            <tbody>
                                <% let counter = 1; %>
                                <% orders.forEach(order => { %>
                                    <tr>
                                        <td><%= counter %></td>
                                        <td><%= order._id %></td>
                                        <td><%= order.user ? order.user.email : 'N/A' %></td>
                                        <td>$<%= order.total.toFixed(2) %></td>
                                        <td>
                                            <span class="badge rounded-pill <% if (order.status === 'Received') { %>alert-success<% } else if (order.status === 'Shipped') { %>alert-info<% } else { %>alert-warning<% } %>">
                                                <%= order.status %>
                                            </span>
                                        </td>
                                        <td><%= order.createdAt.toDateString() %></td>
                                        <td class="text-end">
                                            <a href="/page-orderDetails/<%= order._id %>" class="btn btn-md rounded font-sm">Detail</a>
                                        </td>
                                    </tr>
                                    <% counter++; %>
                                <% }); %>
                            </tbody>
                        </table>
                        
                        
                        
