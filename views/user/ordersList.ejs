    <body>



        <div class="container mt-4">
            <div class="card">
                <div class="card-header bg-primary text-white">
                    <h5 class="mb-0">Your Orders</h5>
                    
                </div>
                <div class="card-body">
                    <div class="table-responsive">

                        <!-- Add SweetAlert2 CDN -->
                        <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

                        <table class="table table-bordered">
                            <thead class="thead-light">
                                <tr>
                                    <th>Order ID</th>
                                    <th>Date</th>
                                    <th>Total</th>
                                    <th>Status</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% if (orders && orders.length> 0) { %>
                                    <% orders.forEach(order=> { %>
                                        <tr>
                                            <td>
                                                <%= order._id %>
                                            </td>
                                            <td>
                                                <%= order.createdAt.toDateString() %>
                                            </td>
                                            <td>$<%= order.total.toFixed(2) %>
                                            </td>
                                            <td>
                                                <%= order.status==='Cancel' ? 'Cancelled' : order.status %>
                                            </td>
                                            <td>
                                                <!-- View Details button -->
                                                <a href="/order-details/<%= order._id %>"
                                                    class="btn btn-info btn-sm mr-2">View Details</a>

                                                <!-- Cancel/Request Return button -->
                                                <% if (order.status==='Delivered' ||order.status==='Returned' ) { %>
                                                    <% if (!order.returnRequested) { %>
                                                     
                                                        <% } else { %>
                                                        
                                                            <% } %>
                                                                <% } else if (order.status !=='Cancel' ) { %>
                                                                    <button class="btn btn-danger btn-sm"
                                                                        onclick="showCancelReasonPrompt('<%= order._id %>')">Cancel
                                                                        Order</button>
                                                                    <% } else { %>
                                                                        <span
                                                                            class="btn btn-danger btn-sm disabled">Cancelled</span>
                                                                        <% } %>
                                            </td>
                                        </tr>
                                        <% }); %>
                                            <% } else { %>
                                                <tr>
                                                    <td colspan="5">No orders available.</td>
                                                </tr>
                                                <% } %>
                            </tbody>
                        </table>

                        <script>
                            async function showCancelReasonPrompt(orderId) {
                                const result = await Swal.fire({
                                    title: 'Provide a reason for cancellation',
                                    input: 'textarea',
                                    inputPlaceholder: 'Type your reason here...',
                                    showCancelButton: true,
                                    confirmButtonText: 'Cancel Order',
                                    preConfirm: (cancelReason) => {
                                        if (!cancelReason) {
                                            Swal.showValidationMessage('Please provide a reason');
                                        }
                                        return cancelReason;
                                    },
                                });

                                if (result.isConfirmed) {
                                    try {
                                        const response = await fetch(`/cancel-order/${orderId}`, {
                                            method: 'POST',
                                            headers: {
                                                'Content-Type': 'application/json',
                                            },
                                            body: JSON.stringify({ cancelReason: result.value }),
                                        });

                                        const data = await response.json();

                                        if (data.message) {
                                            Swal.fire({
                                                title: 'Order Cancelled!',
                                                text: data.message,
                                                icon: 'success',
                                            }).then(() => {
                                                // Reload the page or update the UI as needed
                                                location.reload();
                                            });
                                        } else {
                                            Swal.fire({
                                                title: 'Error!',
                                                text: 'An error occurred while canceling the order.',
                                                icon: 'error',
                                            });
                                        }
                                    } catch (error) {
                                        console.error(error);
                                        Swal.fire({
                                            title: 'Error!',
                                            text: 'An error occurred while canceling the order.',
                                            icon: 'error',
                                        });
                                    }
                                }
                            }

                           
                        </script>


                    </div>
