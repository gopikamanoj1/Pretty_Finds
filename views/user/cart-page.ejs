                                </thead>
                                <tbody>
                                    <% if (products.length> 0) { %>
                                        <% for (let i=0; i < products.length; i++) { %>
                                            
                                                <tr>
                                                    <td class="image">
                                                        <img src="/uploads/<%= products[i].productId.image[0].url %>"
                                                            alt="<%= products[i].name %>">
                                                    </td>
                                                    <td class="product-des">
                                                        <a href="<%= '/singleProduct/' + products[i].productId._id %>">
                                                                <%= products[i].productId.name %>
                                                            </a></h5>
                                                    </td>
                                                    <td class="price" data-title="Price"><span>
                                                            <%= products[i].productId.price %>
                                                        </span></td>

                                                    <td class="text-center" data-title="Stock">

                                                        <button class="btn"
                                                            id="decrement_<%= products[i].productId._id %>"
                                                            onclick="decrementQuantity('<%= products[i].productId._id %>')">-</button>
                                                        <% if (products[i].productId.stock <
                                                            products[i].quantity) { %>
                                                            <h1>Out of Stock</h1>
                                                            <% } else { %>
                                                                <span
                                                                    id="quantity_<%= products[i].productId._id %>"
                                                                    class="mr-4 font-weight-bold"
                                                                    name="quantity" data-min="0"
                                                                    data-max="10">
                                                                    <%= products[i].quantity %>
                                                                </span>
                                                                <% } %>
                                                                    <button class="btn"
                                                                        id="increment_<%= products[i].productId._id %>"
                                                                        onclick="incrementQuantity('<%= products[i].productId._id %>')">+</button>
                                                    </td>


                                                    <td class="text-right" data-title="Cart">
                                                        <% var total=products[i].quantity *
                                                            products[i].productId.price; %>
                                                            <h5> ₹<span
                                                                    id="product-total-price-<%= products[i].productId._id %>"
                                                                    class="item-total">
                                                                    <%= total %>
                                                                </span></h5>
                                                    </td>
                                                    <td class="action" data-title="Remove">
                                                        <form action="/removeProduct" method="POST"
                                                            id="removeProductForm<%= i %>">
                                                            <input type="hidden" name="productId"
                                                                value="<%= products[i].productId._id %>">
                                                            <button type="button" class="text-muted"
                                                                style="border: none; background-color: transparent; cursor: pointer;"
                                                                data-bs-toggle="modal"
                                                                data-bs-target="#confirmRemoveModal<%= i %>">
                                                                <i class="fi-rs-trash"></i>
                                                            </button>

                                                            <div class="modal fade"
                                                                id="confirmRemoveModal<%= i %>"
                                                                tabindex="-1"
                                                                aria-labelledby="confirmRemoveModalLabel"
                                                                aria-hidden="true">
                                                                <div class="modal-dialog">
                                                                    <div class="modal-content">
                                                                        <div class="modal-header">
                                                                            <h5 class="modal-title"
                                                                                id="confirmRemoveModalLabel">
                                                                                Confirmation</h5>
                                                                            <button type="button"
                                                                                class="btn-close"
                                                                                data-bs-dismiss="modal"
                                                                                aria-label="Close"></button>
                                                                        </div>
                                                                        <div class="modal-body">
                                                                            Are you sure you want to remove
                                                                            this product?
                                                                        </div>
                                                                        <div class="modal-footer">
                                                                            <button type="button"
                                                                                class="btn btn-secondary"
                                                                                data-bs-dismiss="modal">Cancel</button>
                                                                            <button type="submit"
                                                                                form="removeProductForm<%= i %>"
                                                                                class="btn btn-danger">Remove</button>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </form>

                                                    </td>

                                                </tr>
                                                
                                            <% } %>
                                                <% } else { %>
                                                    <tr>
                                                        <td colspan="6">No products in the cart</td>
                                                    </tr>
                                                    <% } %>

                                                        
                                </tbody>
                            </table>
                        </div>
                        <div class="cart-action text-end">
                            <a href="/allProducts" class="btn"><i class="fi-rs-shopping-bag mr-10"></i>Continue
                                Shopping</a>
                        </div>

                        <div class="divider center_icon mt-50 mb-50"><i class="fi-rs-fingerprint"></i></div>
                        <div class="row mb-50">
                            
                            
                            <div class="col-lg-6 col-md-12">
                                <div class="border p-md-4 p-30 border-radius cart-totals">
                                    <div class="heading_s1 mb-3">
                                        <h4>Cart Totals</h4>
                                    </div>
                                    <div class="table-responsive">
                                        <table class="table">
                                            <% /* Function to calculate cart total */ %>
                                                <% function calculateCartTotal(products) { %>
                                                    <% let total=0; %>
                                                        <% products.forEach(product=> { %>
                                                            <% total +=product.quantity * product.productId.price; %>
                                                                <% }) %>
                                                                    <% return total; %>
                                                                        <% } %>
                                                                            <% if (products && products.length> 0) { %>
                                                                                    <tbody>
                                                                                        <% products.forEach(product=> { %>

                                                                                            <% }) %>
                                                                                                <tr>
                                                                                                    <td
                                                                                                        class="cart_total_label">
                                                                                                        Cart Subtotal</td>
                                                                                                    <td
                                                                                                        class="cart_total_amount">
                                                                                                        <span id="subtotal"
                                                                                                            class="font-lg fw-900 text-brand totalValue">
                                                                                                            $<%= calculateCartTotal(products).toFixed(2)
                                                                                                                %>
                                                                                                        </span>
                                                                                                    </td>
                                                                                                </tr>
                                                                                              
                                                                                                <tr>
                                                                                                    <td
                                                                                                        class="cart_total_label">
                                                                                                        Shipping</td>
                                                                                                    <td
                                                                                                        class="cart_total_amount">
                                                                                                        <i
                                                                                                            class="ti-gift mr-5"></i>
                                                                                                        Free Shipping
                                                                                                    </td>
                                                                                                </tr>
                                                                                                <tr>
                                                                                                    <td
                                                                                                        class="cart_total_label">
                                                                                                        Category Offer </td>
                                                                                                    <td
                                                                                                        class="cart_total_amount">
                                                                                                        <i
                                                                                                            class="ti-gift mr-5"></i>
                                                                                                         offer 
                                                                                                    </td>
                                                                                                </tr>
                                                                                                <tr>
                                                                                                    <td
                                                                                                        class="cart_total_label">
                                                                                                        Total</td>
                                                                                                    <td
                                                                                                        class="cart_total_amount">
                                                                                                        <span id="subtotal"
                                                                                                            class="font-lg fw-900 text-brand totalValue">
                                                                                                            $<%= calculateCartTotal(products).toFixed(2)
                                                                                                                %>
                                                                                                        </span>
                                                                                                    </td>
                                                                                                </tr>
                                                                                    </tbody>
                                                                                <% } else { %>
                                                                                    <tr>
                                                                                        <td colspan="2">Your cart is
                                                                                            empty</td>
                                                                                    </tr>
                                                                                    <% } %>



                                        </table>
                                    </div>
                                    <% if (!isCartEmpty) { %>
                                        <a href="/checkout-page" class="btn "> <i class="fi-rs-box-alt mr-10"></i>
