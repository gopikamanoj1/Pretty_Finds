                                                                        <button class="cart-item-count mr-4 btn btn-danger" id="decrement_<%= products[i].productId._id %>"
                                                                            onclick="decrementQuantity('<%= products[i].productId._id %>')">-</button>
                                                                            <% if (products[i].productId.stock < products[i].quantity) { %>
                                                                                <h1>Out of Stock</h1>
                                                                            <% } else { %>
                                                                                <span id="quantity_<%= products[i].productId._id %>" class="mr-4 font-weight-bold" name="quantity" data-min="0" data-max="10"><%= products[i].quantity %></span>
                                                                            <% } %>
                                                                            
                                                                            
                                                                      
                                                                        
                                                                            <button class="cart-item-count mr-4 btn btn-danger" id="increment_<%= products[i].productId._id %>"
                                                                                onclick="incrementQuantity('<%= products[i].productId._id %>')">+</button>
                                                                    
                                                
                                                                      </td>


                                                                       

                                                                   
                                                              

                                                                
                                                                    <td class="text-right" data-title="Cart">
                                                                        <% var total = products[i].quantity * products[i].productId.price; %>
                                                                      <h5> ₹<span id="product-total-price-<%= products[i].productId._id %>" class="item-total">
                                                                               <%= total %></span></h5>
                                                                      </td>
                                                             

                                                                <td class="action" data-title="Remove">
                                                                    <form action="/removeProduct" method="POST">
                                                                        <input type="hidden" name="productId" value="<%= products[i].productId._id %>">
                                                                        <button type="submit" class="text-muted" style="border: none; background-color: transparent; cursor: pointer;">
                                                                            <i class="fi-rs-trash"></i>
                                                                        </button>
                                                                    </form>
                                                                </td>
                                                                
                                                            </tr>
                                                            <% } %>
