        function incrementQuantity(cartItemId) {
    $.ajax({
        url: '/increaseQuantity',
        method: "post",
        data: { cartItemId: cartItemId },
        success: function (data) {
            if (data.success) {
                $(`#product-total-price-${cartItemId}`).text(data.total);
                $(`#quantity_${cartItemId}`).text(data?.quantity);
                setSubtotal();
            } else if (data.success === false) {
                if (data.maxQuantity) {
                    // Display SweetAlert for stock limit exceeded
                    Swal.fire({
                        icon: 'info',
                        title: 'Stock Limit Exceeded',
                        text: 'Sorry, you have reached the maximum quantity for this product.',
                        position: 'center',
                        showConfirmButton: false,
                        timer: 3000,
                    });
                }
                setSubtotal();
            } else {
                console.log('error');
            }
        },
        error: function (jqXHR, textStatus, errorThrown) {
            console.error(errorThrown);
        }
    });
}





