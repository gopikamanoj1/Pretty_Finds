    <!-- Include SweetAlert library -->
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>


<script>
    async function requestReturn(orderId) {

        const checkReturnResponse = await fetch(`/check-return-status/${orderId}`);
        const checkReturnData = await checkReturnResponse.json();

        if (!checkReturnData.isReturnRequested) {
            Swal.fire({
                title: 'Return Requested',
                text: 'Return has already been requested for this order.',
                icon: 'info',
            });
            return;
        }

        const { value: reason } = await Swal.fire({
            title: 'Request Return',
            input: 'text',
            inputLabel: 'Reason for Return',
            inputPlaceholder: 'Please provide a reason for returning...',
            showCancelButton: true,
            confirmButtonText: 'Submit Request',
            cancelButtonText: 'Cancel',
            inputValidator: (value) => {
                if (!value) {
                    return 'You need to provide a reason for returning.';
                }
            },
        });

        if (reason) {
            const result = await Swal.fire({
                title: 'Confirm Return Request',
                text: `Are you sure you want to request a return for this order with the reason: ${reason}?`,
                icon: 'question',
                showCancelButton: true,
                confirmButtonText: 'Yes, Request Return',
            });

            if (result.isConfirmed) {
                // Make an API call to request return
                const response = await fetch(`/return-order/${orderId}?reason=${encodeURIComponent(reason)}`, {
                    method: 'POST',
                });

                const data = await response.json();

                if (response.ok) {
                    // Update the UI to hide the "Request Return" button
                    document.querySelector(`#requestReturnButton`).style.display = 'none';
                    // Show the return placed message
                    document.querySelector(`#returnPlacedMessage`).style.display = 'block';

                    // Fetch the current status and amount in the wallet from the server
                    const statusResponse = await fetch(`/get-order-status/${orderId}`);
                    const statusData = await statusResponse.json();

                    // Update the UI with the current status and amount in the wallet
                    document.querySelector(`#currentStatus`).innerText = `Current Status: ${statusData.status}`;
                    document.querySelector(`#amountInWallet`).innerText = `Amount in Wallet: $${statusData.walletAmount.toFixed(2)}`;

                    // For demonstration purposes, let's show a success message
                    Swal.fire({
                        title: 'Return Requested!',
                        text: 'Your return request has been submitted.',
                        icon: 'success',
                    });
                } else {
                    // Handle the case where return request failed
                    Swal.fire({
                        title: 'Error!',
                        text: data.error || 'An error occurred while requesting return.',
                        icon: 'error',
                    });
                }
            }
        }
    }
</script>




